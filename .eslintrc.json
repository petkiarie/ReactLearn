{
    "extends":[
        "eslint:recommended", 
        "plugin:react/recommended",
        "plugin:import/errors",
        "plugin:jsx-a11y/recommended",
        "prettier"],
    "plugins":[
       "react","import","jsx-a11y"
    ],
    "rules": {
        "no-console": "warn",
        "react/react-in-jsx-scope": "off",
        "react/prop-types": "off"
    },
    "parserOptions":{
        "ecmaVersion": 2022,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "env": {
        "browser": true,
        "node": true,
        "es6": true
    },
    "settings":{
        "react": {
            "version": "detect"
        }
    },
    "import/resolver": {
        "node": {
            "extensions": [".js", ".jsx"]
        }
    }

}